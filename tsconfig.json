{
  "include": ["src"],
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020"],
    "types": [],
    "module": "Node16",
    "moduleResolution": "Node16",
    "resolveJsonModule": true,
    "rootDir": "src",
    "noEmit": true,
    "strict": true,
    "noImplicitOverride": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  }
}
