{
  "include": ["src", "resources", "*.d.ts"],
  "compilerOptions": {
    "target": "ES2021",
    "lib": ["ES2021"],
    "types": [],
    "module": "Node16",
    "moduleResolution": "Node16",
    "resolveJsonModule": true,
    "noEmit": true,
    "strict": true,
    "noImplicitOverride": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  }
}
