{
  "name": "my-extension",
  "displayName": "My Extension",
  "description": "Transform (multiple) cursor position or selection to many cases. Cases: `capitalCase`, `camelCase`, `constantCase`, `pascalCase`, `paramCase`, `snakeCase`, `sentenceCase`, `dotCase`, `pathCase`, `headerCase`, `noCase`",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "url": "git+https://github.com/xingqingxyz/my-extension.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/xingqingxyz/my-extension/issues"
  },
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "title": "Transform Case: Title",
        "command": "my-extension.transformToCapitalCase"
      },
      {
        "title": "Transform Case: Camel",
        "command": "my-extension.transformToCamelCase"
      },
      {
        "title": "Transform Case: Constant",
        "command": "my-extension.transformToConstantCase"
      },
      {
        "title": "Transform Case: Pascal",
        "command": "my-extension.transformToPascalCase"
      },
      {
        "title": "Transform Case: Kebab",
        "command": "my-extension.transformToParamCase"
      },
      {
        "title": "Transform Case: Snake",
        "command": "my-extension.transformToSnakeCase"
      },
      {
        "title": "Transform Case: Dot",
        "command": "my-extension.transformToDotCase"
      },
      {
        "title": "Transform Case: Path",
        "command": "my-extension.transformToPathCase"
      },
      {
        "title": "Transform Case: Sentence",
        "command": "my-extension.transformToSentenceCase"
      },
      {
        "title": "Transform Case: No",
        "command": "my-extension.transformToNoCase"
      },
      {
        "title": "Transform Case: Header",
        "command": "my-extension.transformToHeaderCase"
      }
    ],
    "keybindings": [
      {
        "when": "editorTextFocus && !editorReadonly",
        "key": "alt+x alt+c",
        "command": "my-extension.transformToCapitalCase"
      },
      {
        "when": "editorTextFocus && !editorReadonly",
        "key": "alt+x c",
        "command": "my-extension.transformToCamelCase"
      },
      {
        "key": "alt+x shift+c",
        "when": "editorTextFocus && !editorReadonly",
        "command": "my-extension.transformToConstantCase"
      },
      {
        "key": "alt+x p",
        "when": "editorTextFocus && !editorReadonly",
        "command": "my-extension.transformToPascalCase"
      },
      {
        "key": "alt+x k",
        "when": "editorTextFocus && !editorReadonly",
        "command": "my-extension.transformToParamCase"
      },
      {
        "key": "alt+x s",
        "when": "editorTextFocus && !editorReadonly",
        "command": "my-extension.transformToSnakeCase"
      },
      {
        "key": "alt+x .",
        "when": "editorTextFocus && !editorReadonly",
        "command": "my-extension.transformToDotCase"
      },
      {
        "key": "alt+x /",
        "when": "editorTextFocus && !editorReadonly",
        "command": "my-extension.transformToPathCase"
      },
      {
        "key": "alt+x space",
        "when": "editorTextFocus && !editorReadonly",
        "command": "my-extension.transformToSentenceCase"
      },
      {
        "key": "alt+x n",
        "when": "editorTextFocus && !editorReadonly",
        "command": "my-extension.transformToNoCase"
      },
      {
        "key": "alt+x h",
        "when": "editorTextFocus && !editorReadonly",
        "command": "my-extension.transformToHeaderCase"
      },
      {
        "key": "alt+x u",
        "when": "editorTextFocus && !editorReadonly",
        "command": "editor.action.transformToUppercase"
      },
      {
        "key": "alt+x l",
        "when": "editorTextFocus && !editorReadonly",
        "command": "editor.action.transformToLowercase"
      },
      {
        "key": "alt+x alt+oem_3",
        "when": "editorTextFocus && !editorReadonly",
        "command": "editor.action.transposeLetters"
      },
      {
        "key": "alt+oem_3",
        "when": "editorTextFocus && !editorReadonly",
        "command": "editor.action.transpose"
      }
    ]
  },
  "scripts": {
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "vscode:prepublish": "npm run esbuild-base -- --minify",
    "esbuild": "npm run esbuild-base -- --sourcemap",
    "esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
    "esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=out/extension.js --external:vscode --format=cjs --platform=node"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.77.0",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vscode/test-electron": "^2.2.0",
    "esbuild": "^0.17.18",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^5.0.4"
  }
}
