{
  "name": "my-extension",
  "displayName": "My Extension",
  "description": "A pack of my personal using features.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "my-extension.invokePowerShellCommand",
        "title": "Invoke PowerShell Command"
      },
      {
        "command": "my-extension.helloWorld",
        "title": "Hello Wold"
      },
      {
        "command": "my-extension.extended",
        "title": "Hello Extended"
      }
    ]
  },
  "scripts": {
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.77.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "change-case": "^4.1.2"
  }
}
