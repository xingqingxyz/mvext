{
  "name": "mvext",
  "displayName": "My VSCode Extension",
  "description": "Transform (multiple) cursor position or selection to many cases. Cases: `capitalCase`, `camelCase`, `constantCase`, `pascalCase`, `paramCase`, `snakeCase`, `sentenceCase`, `dotCase`, `pathCase`, `headerCase`, `noCase`",
  "version": "0.1.1",
  "l10n": "./l10n",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Programming Languages",
    "Other"
  ],
  "author": {
    "email": "cm.email@qq.com",
    "name": "Ming.Chen.",
    "url": "https://github.com/xingqingxyz"
  },
  "repository": {
    "url": "https://github.com/xingqingxyz/mvext.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/xingqingxyz/mvext/issues"
  },
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "title": "Transform Case: Title",
        "command": "mvext.transformToCapitalCase"
      },
      {
        "title": "Transform Case: Camel",
        "command": "mvext.transformToCamelCase"
      },
      {
        "title": "Transform Case: Constant",
        "command": "mvext.transformToConstantCase"
      },
      {
        "title": "Transform Case: Pascal",
        "command": "mvext.transformToPascalCase"
      },
      {
        "title": "Transform Case: Kebab",
        "command": "mvext.transformToParamCase"
      },
      {
        "title": "Transform Case: Snake",
        "command": "mvext.transformToSnakeCase"
      },
      {
        "title": "Transform Case: Dot",
        "command": "mvext.transformToDotCase"
      },
      {
        "title": "Transform Case: Path",
        "command": "mvext.transformToPathCase"
      },
      {
        "title": "Transform Case: Sentence",
        "command": "mvext.transformToSentenceCase"
      },
      {
        "title": "Transform Case: No",
        "command": "mvext.transformToNoCase"
      },
      {
        "title": "Transform Case: Header",
        "command": "mvext.transformToHeaderCase"
      },
      {
        "title": "Transform Case: Lower",
        "command": "mvext.transformToLowerCase"
      },
      {
        "title": "Transform Case: Upper",
        "command": "mvext.transformToUpperCase"
      },
      {
        "title": "Detect Transform Case",
        "command": "mvext.detectTransformCase"
      },
      {
        "title": "mvext.myCommand",
        "command": "mvext.myCommand"
      },
      {
        "title": "Switch File: HTML/JS/CSS/TS",
        "command": "mvext.switchFile"
      }
    ],
    "keybindings": [
      {
        "when": "editorTextFocus && !editorReadonly",
        "key": "alt+x alt+c",
        "command": "mvext.transformToCapitalCase"
      },
      {
        "when": "editorTextFocus && !editorReadonly",
        "key": "alt+x c",
        "command": "mvext.transformToCamelCase"
      },
      {
        "key": "alt+x shift+c",
        "when": "editorTextFocus && !editorReadonly",
        "command": "mvext.transformToConstantCase"
      },
      {
        "key": "alt+x p",
        "when": "editorTextFocus && !editorReadonly",
        "command": "mvext.transformToPascalCase"
      },
      {
        "key": "alt+x k",
        "when": "editorTextFocus && !editorReadonly",
        "command": "mvext.transformToParamCase"
      },
      {
        "key": "alt+x s",
        "when": "editorTextFocus && !editorReadonly",
        "command": "mvext.transformToSnakeCase"
      },
      {
        "key": "alt+x .",
        "when": "editorTextFocus && !editorReadonly",
        "command": "mvext.transformToDotCase"
      },
      {
        "key": "alt+x /",
        "when": "editorTextFocus && !editorReadonly",
        "command": "mvext.transformToPathCase"
      },
      {
        "key": "alt+x space",
        "when": "editorTextFocus && !editorReadonly",
        "command": "mvext.transformToSentenceCase"
      },
      {
        "key": "alt+x n",
        "when": "editorTextFocus && !editorReadonly",
        "command": "mvext.transformToNoCase"
      },
      {
        "key": "alt+x h",
        "when": "editorTextFocus && !editorReadonly",
        "command": "mvext.transformToHeaderCase"
      },
      {
        "key": "alt+x u",
        "when": "editorTextFocus && !editorReadonly",
        "command": "mvext.transformToUpperCase"
      },
      {
        "key": "alt+x l",
        "when": "editorTextFocus && !editorReadonly",
        "command": "mvext.transformToLowerCase"
      },
      {
        "key": "alt+x alt+d",
        "when": "editorTextFocus && !editorReadonly",
        "command": "mvext.detectTransformCase"
      },
      {
        "key": "alt+x alt+e",
        "command": "mvext.myCommand"
      },
      {
        "key": "alt+o",
        "command": "mvext.switchFile"
      }
    ],
    "problemMatchers": [
      {
        "name": "esbuild",
        "label": "esbuild problem matcher",
        "severity": "error",
        "applyTo": "closedDocuments",
        "source": "esbuild",
        "fileLocation": "relative",
        "owner": "typescript",
        "pattern": [
          {
            "regexp": "> (.*?):([0-9]+):([0-9]+): (warning|error): (.+)$",
            "file": 1,
            "line": 2,
            "column": 3,
            "severity": 4,
            "message": 5
          }
        ]
      },
      {
        "name": "esbuild-watch",
        "base": "$esbuild",
        "label": "esbuild --watch problem matcher",
        "severity": "error",
        "applyTo": "closedDocuments",
        "source": "esbuild",
        "owner": "typescript",
        "fileLocation": "relative",
        "background": {
          "activeOnStart": true,
          "beginsPattern": {
            "regexp": "\\[watch\\] build started"
          },
          "endsPattern": {
            "regexp": "\\[watch\\] build finished, watching for changes..."
          }
        }
      }
    ]
  },
  "scripts": {
    "dev": "esbuild ./src/extension.ts --outfile=out/extension.js --external:vscode --format=cjs --platform=node --bundle --watch",
    "vscode:test": "tsc && node ./out/test/runTest.js",
    "eslint": "eslint src --ext ts --fix --cache",
    "prettier": "prettier -w . --cache",
    "stage": "cross-env NODE_ENV=stage pnpm eslint && pnpm prettier && pwsh install.ps1",
    "vscode:prepublish": "esbuild ./src/extension.ts --outfile=out/extension.js --external:vscode --format=cjs --platform=node --bundle --minify"
  },
  "devDependencies": {
    "@types/glob": "8.1.0",
    "@types/mocha": "10.0.1",
    "@types/node": "^20.5.0",
    "@types/vscode": "1.81.0",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "@vscode/test-electron": "2.3.4",
    "esbuild": "0.19.2",
    "eslint": "8.45.0",
    "glob": "10.3.3",
    "mocha": "10.2.0",
    "typescript": "5.1.6"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "bracketSameLine": true
  }
}
